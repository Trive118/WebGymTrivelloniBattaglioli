@model WebGymTrivelloniBattaglioli.Models.TrainerModel

@{
    ViewBag.Title = "Contact";
}

<html>
<head>
    <link rel="stylesheet" type="text/css" href="../Content/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="../Content/font-awesome.css">
    <link rel="stylesheet" href="../Content/templatemo-training-studio.css">
</head>
<!-- ***** Testimonials Starts ***** -->
<body>
    <section class="section" id="trainers">
        <div class="container">
            <div class="row">
                <div class="col-lg-6 offset-lg-3">
                    <div class="section-heading">
                        <h2>Il nostro <em>Team</em></h2>
                        <img src="../Images/line-dec.png" alt="">
                        <p>Una palestra fatta di grandi persone.</p>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-4">
                    <div class="trainer-item">
                        <div class="image-thumb">
                            <img src="../Images/first-trainer.jpg" alt="">
                        </div>
                        <div class="down-content">
                            <span>Personal Trainer</span>
                            <h4>Marco Bonfiglio</h4>
                            <p>10 anni di esperienza in bodybuilding. Marco ti aspetta in palestra per iniziare il tuo cambiamento.</p>
                        </div>
                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="trainer-item">
                        <div class="image-thumb">
                            <img src="../Images/zumba-trainer-new.jpg" alt="">
                        </div>
                        <div class="down-content">
                            <span>Zumba Master</span>
                            <h4>Michela Fiore</h4>
                            <p>Energia, carisma e tanta voglia di lavorare: Michela ti seguirà nelle lezioni di zumba per tenere i ritmi alti.</p>
                        </div>
                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="trainer-item">
                        <div class="image-thumb">
                            <img src="../Images/yoga-teacher-new.jpg" alt="">
                        </div>
                        <div class="down-content">
                            <span>Yoga Master</span>
                            <h4>Camilla De Padovanis</h4>
                            <p>Camilla lavora assieme a WebGym per offrirti il massimo relax nelle sue lezioni di Yoga.</p>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- ***** Testimonials Ends ***** -->
    <!-- ***** Contact Us Area Starts ***** -->
    <section class="section" id="contact-us">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-6 col-md-6 col-xs-12">
                    <div id="map">
                        <iframe src="https://maps.google.com/maps?width=100%25&amp;height=600&amp;hl=en&amp;q=universit%C3%A0%20di%20parma+(My%20Business%20Name)&amp;t=&amp;z=14&amp;ie=UTF8&amp;iwloc=B&amp;output=embed" width="100%" height="440px" frameborder="0" style="border:0" allowfullscreen></iframe>
                    </div>
                </div>

                <div class="col-lg-6 col-md-6 col-xs-12">
                    <div class="contact-form">
                        <form id="contact" action="" method="post">
                            <h4>Area <um>Personal Trainer</um></h4>
                            <div class="row">
                                <div class="col-md-12 col-sm-12">
                                <fieldset>
                                <input name="email" type="text" id="email" placeholder="Email*" required="">
                                </fieldset> 
                                </div>
                                <div class="col-md-12 col-sm-12">
                                <fieldset>
                                <input name="password" type="text" id="password" placeholder="Password*" required="">
                                </fieldset>
                                </div>
                                <div class="col-lg-12">
                                <fieldset>
                                <button type="submit" id="form-submit" class="main-button">Entra</button>
                                </fieldset>
                                </div>
                            </div>
                        </form>

                    </div>
                </div>
            </div>
        </div>
    </section>

</body>
<!-- ***** Contact Us Area Ends ***** -->

</html>